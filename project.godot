; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=4

_global_script_classes=[ {
"base": "Node2D",
"class": "LevelManager",
"language": "GDScript",
"path": "res://LevelManager.gd"
}, {
"base": "Node",
"class": "PlayerModel",
"language": "GDScript",
"path": "res://PlayerModel.gd"
} ]
_global_script_class_icons={
"LevelManager": "",
"PlayerModel": ""
}

[application]

config/name="Platformer"
run/main_scene="res://StartMenu.tscn"
config/use_custom_user_dir=true
config/custom_user_dir_name=".platformer"
config/icon="res://icon.png"

[autoload]

SignalBus="*res://SignalBus.gd"
PlayerModel="*res://PlayerModel.gd"
SceneLoader="*res://SceneLoader.gd"
LocalizationHandler="*res://LocalizationHandler.gd"

[display]

window/size/width=1280
window/size/height=720
window/stretch/mode="2d"
window/stretch/aspect="keep"

[input]

lang_switch={
"deadzone": 0.5,
"events": [ Object(InputEventKey,"resource_local_to_scene":false,"resource_name":"","device":0,"alt":false,"shift":false,"control":true,"meta":false,"command":true,"pressed":false,"scancode":76,"unicode":0,"echo":false,"script":null)
 ]
}

[locale]

translations=PoolStringArray( "res://Lang/translations.de.translation", "res://Lang/translations.en.translation" )
locale_filter=[ 1, [ "de", "en" ] ]

[logging]

file_logging/enable_file_logging=true

[rendering]

quality/driver/driver_name="GLES2"
quality/2d/use_pixel_snap=true
vram_compression/import_etc=true
vram_compression/import_etc2=false
environment/default_environment="res://default_env.tres"
